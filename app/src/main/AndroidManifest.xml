<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nl.tue.thermostat">

    <uses-permission android:name="android.permission.INTERNET"> </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ThermostatOverview"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WeekOverview"
            android:label="Week overview" />
        <activity
            android:name=".SetTemperature"
            android:label="Set temperatures" />
        <activity
            android:name=".Monday"
            android:label="Monday" />
        <activity
            android:name=".Tuesday"
            android:label="Tuesday" />
        <activity
            android:name=".Wednesday"
            android:label="Wednesday" />
        <activity
            android:name=".Thursday"
            android:label="Thursday" />
        <activity
            android:name=".Friday"
            android:label="Friday" />
        <activity
            android:name=".Saturday"
            android:label="Saturday"/>
        <activity
            android:name=".Sunday"
            android:label="Sunday"></activity>
    </application>

</manifest>